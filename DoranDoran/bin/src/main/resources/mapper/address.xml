<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="address">
	<select id="cityList" resultType="Address">
		SELECT DISTINCT city FROM address
	</select>	
	<select id="guList" resultType="String">
		SELECT DISTINCT gu FROM address
		WHERE city = #{city}
	</select>	
	
	<insert id="add">
		INSERT INTO address (city, gu)
		VALUES (#{city}, #{gu})
	</insert>
	
</mapper>